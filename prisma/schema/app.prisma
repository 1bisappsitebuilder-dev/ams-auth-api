model App {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  description String
  icon        String?
  code        String   @unique
  withModule Boolean
  isDeleted   Boolean  @default(false)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  modules     Module[]
  organizations OrganizationApp[]
}
